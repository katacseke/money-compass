    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.441.0",
    "next": "^14.2.5",
    "next-cache-toolbar": "^0.2.1",
