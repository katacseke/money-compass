    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.0",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.13.3",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9.9.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.5.0",
