    "react-hook-form": "^7.52.2",
    "react-number-format": "^5.4.0",
    "react-resizable-panels": "^2.1.0",
    "react-responsive": "^10.0.0",
    "recharts": "^2.13.0-alpha.5",
    "server-only": "^0.0.1",
